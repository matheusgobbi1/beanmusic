{
  "cli": {
    "version": ">= 16.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://beanmusicpromotion.com/api/v1"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://beanmusicpromotion.com/api/v1"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://beanmusicpromotion.com/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
